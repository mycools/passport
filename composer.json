{
    "name": "mycools/passport",
    "description": "Laravel Passport provides OAuth2 server support to Laravel.",
    "keywords": ["laravel", "passport", "oauth"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/mycools/passport/issues",
        "source": "https://github.com/mycools/passport"
    },
    "authors": [
        {
            "name": "Jarak Kritkiattisak",
            "email": "jarak.krit@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "firebase/php-jwt": "~3.0|~4.0|~5.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/auth": "~5.6",
        "illuminate/console": "~5.6",
        "illuminate/container": "~5.6",
        "illuminate/contracts": "~5.6",
        "illuminate/database": "~5.6",
        "illuminate/encryption": "~5.6",
        "illuminate/http": "~5.6",
        "illuminate/support": "~5.6",
        "league/oauth2-server": "^7.0",
        "phpseclib/phpseclib": "^2.0",
        "symfony/psr-http-message-bridge": "~1.0",
        "zendframework/zend-diactoros": "~1.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Mycools\\Passport\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0-dev"
        },
        "laravel": {
            "providers": [
                "Mycools\\Passport\\PassportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
